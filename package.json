{
  "name": "cygnus-ngx-ui-kit",
  "version": "1.0.2-20260225",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "build:lib": "npm run build:lib:compile && npm run build:lib:styles",
    "build:lib:compile": "ng build ngx-cygnus-ui --configuration production",
    "build:lib:styles": "cd projects/ngx-cygnus-ui && postcss styles.css -o ../../dist/ngx-cygnus-ui/styles.css --verbose",
    "watch:lib": "concurrently \"ng build ngx-cygnus-ui --watch\" \"npm run watch:lib:styles\"",
    "watch:lib:styles": "postcss projects/ngx-cygnus-ui/styles.css --dir dist/ngx-cygnus-ui --base projects/ngx-cygnus-ui --watch",
    "pack:lib": "npm run build:lib && cd dist/ngx-cygnus-ui && npm pack",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@cygnus/ngx-cygnus-icons": "file:cygnus-ngx-cygnus-icons-1.0.1.tgz",
    "highlight.js": "^11.11.1",
    "ngx-highlightjs": "12.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.17",
    "@angular/cli": "^19.0.4",
    "@angular/compiler-cli": "^19.0.0",
    "@tailwindcss/postcss": "^4.0.0-alpha.30",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.21",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.2.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.0.0-alpha.30",
    "typescript": "~5.6.2"
  }
}
